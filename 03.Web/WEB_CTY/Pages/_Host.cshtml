@page "/"
@namespace WEB_CTY.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>THEWOLDLINK</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="WEB_CTY.styles.css" rel="stylesheet" />
    <link href="../css/hethong.css" rel="stylesheet" />     
    <link rel="icon" href="../images/hethong/logo.png" type="image/gif" sizes="16x16">    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <link href="../jquery/aos.css" rel="stylesheet" />
</head>
<body>
    <component type="typeof(App)" render-mode="ServerPrerendered" />

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>
    <script src="~/jquery/jquery.js"></script>
    <script src="~/jquery/jquery.min.js"></script>    
     
    @*<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>*@
    <script src="../jquery/aos.js"></script>
    <script src="_framework/blazor.server.js"></script>
    <script>
        AOS.init();
    </script>
</body>
</html>
